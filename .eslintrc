{
  "root": true,
  "env": {
    "jest": true
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": ["./tsconfig.json"]
  },
  "extends": ["airbnb-typescript/base", "prettier"],
  "plugins": ["import", "@typescript-eslint"],
  "rules": {
    "comma-dangle": 0,
    "import/extensions": 0
  },
  "settings": {
    "import/parsers": {
      "@typescript-eslint/parser": [".ts"]
    },
    "import/resolver": {
      "typescript": {}
    }
  }
}
